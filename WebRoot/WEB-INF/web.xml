<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	
	<filter>
  <filter-name>setCharacterEncoding</filter-name>
  <filter-class>common.EncodingFilter</filter-class>
  <init-param>
     <param-name>encoding</param-name>
     <param-value>UTF-8</param-value>
  </init-param>
</filter>

<filter-mapping>
	<filter-name>setCharacterEncoding</filter-name>
	<url-pattern>/*</url-pattern>
</filter-mapping>


	<servlet>
		<servlet-name>ResourceTypeServlet</servlet-name>
		<servlet-class>servlet.ResourceTypeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResourceTypeServlet</servlet-name>
		<url-pattern>/ResourceTypeServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResourceTypeServlet</servlet-name>
		<url-pattern>/ResourceTypeServlet/m/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ResourceTypeAjaxServlet</servlet-name>
		<servlet-class>servlet.ResourceTypeAjaxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResourceTypeAjaxServlet</servlet-name>
		<url-pattern>/ResourceTypeAjaxServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResourceTypeAjaxServlet</servlet-name>
		<url-pattern>/ResourceTypeAjaxServlet/m/*</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ResourceAjaxServlet</servlet-name>
		<servlet-class>servlet.ResourceAjaxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResourceAjaxServlet</servlet-name>
		<url-pattern>/ResourceAjaxServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResourceAjaxServlet</servlet-name>
		<url-pattern>/ResourceAjaxServlet/m/*</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ResourceServlet</servlet-name>
		<servlet-class>servlet.ResourceServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResourceServlet</servlet-name>
		<url-pattern>/ResourceServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResourceServlet</servlet-name>
		<url-pattern>/ResourceServlet/m/*</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>TgAjaxServlet</servlet-name>
		<servlet-class>servlet.TgAjaxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TgAjaxServlet</servlet-name>
		<url-pattern>/TgAjaxServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TgAjaxServlet</servlet-name>
		<url-pattern>/TgAjaxServlet/m/*</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>TgServlet</servlet-name>
		<servlet-class>servlet.TgServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TgServlet</servlet-name>
		<url-pattern>/TgServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TgServlet</servlet-name>
		<url-pattern>/TgServlet/m/*</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>ResourceTgAjaxServlet</servlet-name>
		<servlet-class>servlet.ResourceTgAjaxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResourceTgAjaxServlet</servlet-name>
		<url-pattern>/ResourceTgAjaxServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResourceTgAjaxServlet</servlet-name>
		<url-pattern>/ResourceTgAjaxServlet/m/*</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ResourceTgServlet</servlet-name>
		<servlet-class>servlet.ResourceTgServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResourceTgServlet</servlet-name>
		<url-pattern>/ResourceTgServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ResourceTgServlet</servlet-name>
		<url-pattern>/ResourceTgServlet/m/*</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>add.jsp</welcome-file>
	</welcome-file-list>
</web-app>